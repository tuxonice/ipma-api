{
    "name": "tuxonice/ipma-api",
    "description": "PHP Package to manage IPMA API (https://api.ipma.pt/)",
    "keywords" : [
        "api"
    ],
    "license" : "MIT",
    "authors": [
        {
            "name": "Helder Correia",
            "email": "tuxboy@tlab.pt",
            "role" : "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tlab\\IpmaApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tlab\\Tests\\": "tests/Unit"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "twig/twig": "^3.5",
        "opis/json-schema": "^2.3",
        "symfony/http-client": "^7.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.2",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1"
    },
    "minimum-stability" : "stable"
}
